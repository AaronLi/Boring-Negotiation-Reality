   "ReaderIcon": "A BNR\\hoodhood.png",
   "Text": "Once upon a time...",
   "Background": "A BNR\\other\\haha.jpg",
   "Font": "basis33.ttf",
   "FadeInTime": 3,
   "FadeOutTime": 0.5,
